{
    "name": "lightdash",
    "version": "1.2.1",
    "description": "An utility library designed to be lightweight",
    "main": "dist/lightdash.common.js",
    "module": "dist/lightdash.esm.js",
    "constants": {
        "dirBase": {
            "input": "./src",
            "output": "./dist"
        },
        "js": {
            "input": "main",
            "namespace": {
                "module": "lightdash",
                "file": "lightdash"
            }
        }
    },
    "scripts": {
        "start": "npm-run-all lint build test",
        "build": "node tasks/jsDev.js",
        "dist": "node tasks/jsDist.js",
        "lint": "eslint src/",
        "test": "jasmine"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/FelixRilling/lightdash.git"
    },
    "keywords": [
        "lightdash",
        "utility"
    ],
    "author": "Felix Rilling",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/FelixRilling/lightdash/issues"
    },
    "homepage": "https://github.com/FelixRilling/lightdash#readme",
    "devDependencies": {
        "babel-core": "^7.0.0-alpha.18",
        "babel-preset-es2016": "^7.0.0-alpha.18",
        "eslint": "^4.4.0",
        "eslint-plugin-unicorn": "^2.1.2",
        "jasmine": "^2.7.0",
        "jsdoc": "^3.5.4",
        "npm-run-all": "^4.0.2",
        "rollup": "^0.45.2",
        "rollup-plugin-typescript": "^0.8.1",
        "typescript": "^2.4.2",
        "uglify-es": "^3.0.27"
    },
    "dependencies": {}
}
