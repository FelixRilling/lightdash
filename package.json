{
    "name": "lightdash",
    "version": "13.0.0-3",
    "description": "A lightweight TypeScript utility library to complement lodash",
    "main": "./dist/cjs/main.js",
    "module": "./dist/esm/main.js",
    "types": "./dist/esm/main.d.ts",
    "directories": {
        "lib": "./src/",
        "test": "./spec/",
        "doc": "./docs/"
    },
    "scripts": {
        "start": "npm run dist",
        "dist": "npm run clean && npm run lint && npm run build && npm run test && npm run docs",
        "clean": "rimraf ./dist ./spec/dist ./docs",
        "build": "tsc --build tsconfig.json",
        "lint": "eslint ./src/ ./spec/ --ext .ts",
        "fix": "eslint ./src/ ./spec/ --ext .ts --fix",
        "test": "ts-node --project spec/tsconfig.spec.json node_modules/jasmine/bin/jasmine --config=jasmine.json",
        "docs": "typedoc ./src/"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/FelixRilling/lightdash.git"
    },
    "keywords": [
        "lightdash",
        "utility"
    ],
    "author": "Felix Rilling",
    "contributors": [
        "Felix Rilling",
        "Tyler Roberts"
    ],
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/FelixRilling/lightdash/issues"
    },
    "homepage": "https://github.com/FelixRilling/lightdash#readme",
    "dependencies": {
        "@types/lodash": "^4.14.153",
        "lodash": "^4.17.15",
        "lodash-es": "^4.17.15"
    },
    "devDependencies": {
        "@types/jasmine": "^3.5.10",
        "@typescript-eslint/eslint-plugin": "^3.1.0",
        "@typescript-eslint/parser": "^3.1.0",
        "eslint": "^7.1.0",
        "eslint-config-prettier": "^6.11.0",
        "eslint-plugin-import": "^2.20.2",
        "eslint-plugin-prettier": "^3.1.3",
        "jasmine": "^3.5.0",
        "prettier": "^2.0.5",
        "rimraf": "^3.0.2",
        "ts-mockito": "^2.5.0",
        "ts-node": "^8.10.2",
        "typedoc": "^0.17.7",
        "typescript": "^3.9.5"
    }
}
