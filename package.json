{
    "name": "lightdash",
    "version": "11.1.2",
    "description": "A lightweight TypeScript utility library to complement lodash",
    "main": "./dist/lightdash.common.js",
    "module": "./dist/esm/src/main.js",
    "types": "./dist/esm/src/main.d.ts",
    "rollupOptions": {
        "moduleName": "l_",
        "outputName": "lightdash",
        "external": [
            "lodash"
        ],
        "globals": {
            "lodash": "_"
        }
    },
    "scripts": {
        "start": "npm run dist",
        "dist": "npm-run-all clean lint build test docs",
        "clean": "rimraf ./dist ./spec/dist ./docs",
        "build": "npm-run-all build:tsc build:bundle",
        "build:tsc": "tsc --build ./tsconfig.json",
        "build:bundle": "rollup -c rollup.config.js",
        "watch": "tsc --build ./tsconfig.json -w",
        "lint": "eslint ./src/ ./spec/src/ --ext .ts",
        "fix": "eslint ./src/ ./spec/src/ --ext .ts --fix",
        "test": "jasmine",
        "docs": "jsdoc -c jsdoc.json"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/FelixRilling/lightdash.git"
    },
    "keywords": [
        "lightdash",
        "utility"
    ],
    "author": "Felix Rilling",
    "contributors": [
        "Felix Rilling",
        "Tyler Roberts"
    ],
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/FelixRilling/lightdash/issues"
    },
    "homepage": "https://github.com/FelixRilling/lightdash#readme",
    "dependencies": {
        "@types/lodash": "^4.14.149",
        "lodash": "^4.17.15"
    },
    "devDependencies": {
        "@types/jasmine": "^3.5.0",
        "@typescript-eslint/eslint-plugin": "^2.13.0",
        "@typescript-eslint/parser": "^2.13.0",
        "docdash": "^1.1.1",
        "eslint": "^6.8.0",
        "eslint-config-prettier": "^6.9.0",
        "eslint-plugin-prettier": "^3.1.2",
        "jasmine": "^3.5.0",
        "jsdoc": "^3.6.3",
        "npm-run-all": "^4.1.5",
        "prettier": "^1.19.1",
        "rimraf": "^3.0.0",
        "rollup": "^1.27.14",
        "rollup-plugin-node-resolve": "^5.2.0",
        "typescript": "^3.7.4"
    }
}
