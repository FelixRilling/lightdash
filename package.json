{
    "name": "lightdash",
    "version": "12.0.0",
    "description": "A lightweight TypeScript utility library to complement lodash",
    "main": "./dist/lightdash.common.js",
    "module": "./dist/esm/src/main.js",
    "types": "./dist/esm/src/main.d.ts",
    "rollupOptions": {
        "moduleName": "l_",
        "outputName": "lightdash",
        "external": [
            "lodash"
        ],
        "globals": {
            "lodash": "_"
        }
    },
    "scripts": {
        "start": "npm run dist",
        "dist": "npm-run-all lint build test docs",
        "build": "npm-run-all clean:dist build:tsc build:bundle",
        "build:tsc": "tsc --build ./tsconfig.json",
        "build:bundle": "rollup -c rollup.config.js",
        "watch": "tsc --build ./tsconfig.json -w",
        "lint": "eslint ./src/ ./spec/src/ --ext .ts",
        "fix": "eslint ./src/ ./spec/src/ --ext .ts --fix",
        "test": "npm-run-all clean:test build:tsc test:jasmine",
        "test:jasmine": "jasmine --require=source-map-support/register",
        "docs": "npm-run-all clean:docs docs:typedoc",
        "docs:typedoc": "typedoc ./src/",
        "clean:dist": "rimraf ./dist",
        "clean:test": "rimraf ./spec/dist ",
        "clean:docs": "rimraf ./docs"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/FelixRilling/lightdash.git"
    },
    "keywords": [
        "lightdash",
        "utility"
    ],
    "author": "Felix Rilling",
    "contributors": [
        "Felix Rilling",
        "Tyler Roberts"
    ],
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/FelixRilling/lightdash/issues"
    },
    "homepage": "https://github.com/FelixRilling/lightdash#readme",
    "dependencies": {
        "@types/lodash": "^4.14.150",
        "lodash": "^4.17.15"
    },
    "devDependencies": {
        "@rollup/plugin-node-resolve": "^7.1.3",
        "@types/jasmine": "^3.5.10",
        "@typescript-eslint/eslint-plugin": "^2.29.0",
        "@typescript-eslint/parser": "^2.29.0",
        "eslint": "^6.8.0",
        "eslint-config-prettier": "^6.11.0",
        "eslint-plugin-import": "^2.20.2",
        "eslint-plugin-prettier": "^3.1.3",
        "jasmine": "^3.5.0",
        "npm-run-all": "^4.1.5",
        "prettier": "^2.0.5",
        "rimraf": "^3.0.2",
        "rollup": "^2.7.2",
        "source-map-support": "^0.5.19",
        "ts-mockito": "^2.5.0",
        "typedoc": "^0.17.4",
        "typescript": "^3.8.3"
    }
}
