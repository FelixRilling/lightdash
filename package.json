{
    "name": "lightdash",
    "version": "11.0.0",
    "description": "A lightweight TypeScript utility library to complement lodash",
    "main": "./dist/cjs/main.js",
    "module": "./dist/esm/main.js",
    "types": "./dist/types/main.d.ts",
    "rollupOptions": {
        "moduleName": "l_",
        "outputName": "lightdash",
        "external": [
            "lodash"
        ],
        "globals": {
            "lodash": "_"
        }
    },
    "scripts": {
        "start": "npm run dist",
        "build": "npm-run-all -p build:esm build:misc",
        "build:esm": "tsc --build tsconfig.json",
        "build:misc": "npm-run-all build:cjs build:iife",
        "build:cjs": "tsc --build tsconfig.cjs.json",
        "build:iife": "rollup -c rollup.config.iife.js",
        "build:test": "tsc --build tsconfig.test.json",
        "dist": "npm-run-all lint test build docs",
        "lint": "eslint ./src/**/*.ts ./spec/**/*.ts --ext .js,.ts",
        "fix": "eslint ./src/**/*.ts ./spec/**/*.ts --ext .js,.ts --fix",
        "test": "npm-run-all build:test test:jasmine",
        "test:jasmine": "jasmine",
        "docs": "jsdoc -c jsdoc.json"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/FelixRilling/lightdash.git"
    },
    "keywords": [
        "lightdash",
        "utility"
    ],
    "author": "Felix Rilling",
    "contributors": [
        "Felix Rilling",
        "Tyler Roberts"
    ],
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/FelixRilling/lightdash/issues"
    },
    "homepage": "https://github.com/FelixRilling/lightdash#readme",
    "dependencies": {
        "@types/lodash": "^4.14.136",
        "lodash": "^4.17.15"
    },
    "devDependencies": {
        "@types/jasmine": "^3.4.2",
        "@typescript-eslint/eslint-plugin": "^2.3.2",
        "@typescript-eslint/parser": "^2.3.2",
        "docdash": "^1.1.1",
        "eslint": "^6.5.1",
        "eslint-config-prettier": "^6.4.0",
        "eslint-plugin-prettier": "^3.1.1",
        "jasmine": "^3.5.0",
        "jsdoc": "^3.6.3",
        "npm-run-all": "^4.1.5",
        "prettier": "^1.18.2",
        "rollup": "^1.23.1",
        "rollup-plugin-node-resolve": "^5.2.0",
        "typescript": "^3.6.2"
    }
}
