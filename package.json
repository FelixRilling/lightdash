{
    "name": "lightdash",
    "version": "2.0.2",
    "description": "An utility library designed to be lightweight",
    "main": "dist/lightdash.common.js",
    "module": "dist/lightdash.esm.js",
    "constants": {
        "dirBase": {
            "input": "./src",
            "output": "./dist"
        },
        "js": {
            "input": "main",
            "namespace": {
                "module": "lightdash",
                "file": "lightdash"
            }
        }
    },
    "scripts": {
        "start": "npm-run-all lint build test",
        "build": "node tasks/jsDev.js",
        "dist": "node tasks/jsDist.js",
        "test": "jasmine",
        "docs": "jsdoc dist/lightdash.esm.js --destination ./docs"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/FelixRilling/lightdash.git"
    },
    "keywords": [
        "lightdash",
        "utility"
    ],
    "author": "Felix Rilling",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/FelixRilling/lightdash/issues"
    },
    "homepage": "https://github.com/FelixRilling/lightdash#readme",
    "devDependencies": {
        "babel-core": "^7.0.0-beta",
        "babel-preset-env": "^1.6.0",
        "jasmine": "^2.8.0",
        "npm-run-all": "^4.1.1",
        "rollup": "^0.50.0",
        "rollup-plugin-typescript": "^0.8.1",
        "tslint": "^5.7.0",
        "typescript": "^2.5.2",
        "uglify-es": "^3.1.1"
    },
    "dependencies": {}
}
